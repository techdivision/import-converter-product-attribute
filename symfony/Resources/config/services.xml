<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="import_converter_product_attribute.listeners.reduce.attribute.option.value" class="TechDivision\Import\Converter\Product\Attribute\Listeners\ReduceProductAttributeOptionValueListener">
            <argument type="service" id="import.processor.registry"/>
        </service>

        <service id="import_converter_product_attribute.observer.product.to.attribute.option.value" class="TechDivision\Import\Converter\Product\Attribute\Observers\ProductToAttributeOptionValueConverterObserver">
            <argument type="service" id="import.processor.import"/>
            <argument type="service" id="import_attribute.processor.attribute.bunch"/>
            <argument type="service" id="import_attribute.callback.swatch.type.loader"/>
        </service>

        <service id="import_converter_product_attribute.subject.converter" class="TechDivision\Import\Converter\Product\Attribute\Subjects\ConverterSubject" shared="false">
            <argument type="service" id="import.processor.registry"/>
            <argument type="service" id="import.generator.core.config.data.uid"/>
            <argument type="service" id="loggers"/>
            <argument type="service" id="import.events.emitter"/>
        </service>
    </services>

</container>